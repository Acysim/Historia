/**
 * 内置代码
 * 请勿修改
 */
window.projectConfigLoadedHooks = [];
/**
 * * 内置代码
 */
window.configServer = "https://devp.cjlu.edu.cn:5007/config";

/**
 * 移除下面的单行注释可修改全局配置
 */
// window.projectConfigLoadedHooks.push((config) => {
//   debugger;
//   console.log(config);
//   config.oidcSettings.acr_values = "idp:Weixin";
//   console.log(config);
//   return config;
// });


window.projectConfigLoadedHooks.push((config) => {
    //debugger;
    console.log(config);
    var r = "idp:Platform";
    if(location.href.toLocaleLowerCase().includes('ssotest')){
        r = "";
    }else  if (isDingtalkEnv()) {
        r = "idp:DingTalk";
    }
    else if (isWeiXinEnv()) {
            r = "idp:Weixin";
    }
    config.oidcSettings.acr_values = r;
    return config;
});

function isWeiXinEnv() {
    return window.navigator.userAgent
        .toLocaleLowerCase()
        .includes("micromessenger");
}

function isDingtalkEnv() {
    return window.navigator.userAgent.toLocaleLowerCase().includes("dingtalk");
}